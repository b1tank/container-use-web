{
    "name": "cuweb",
    "version": "0.0.2",
    "description": "Container Use Web - A CLI tool for monitoring and managing Container Use environments via a web interface.",
    "type": "module",
    "bin": {
        "cuweb": "./bin/cli.js"
    },
    "scripts": {
        "build": "./scripts/build.sh",
        "build:cli": "node build-cli.mjs",
        "build:frontend": "cd frontend && pnpm run build",
        "build:backend": "cd backend && pnpm run build",
        "clean:build": "rm -rf frontend/dist backend/dist bin",
        "clean:deps": "rm -rf node_modules frontend/node_modules backend/node_modules",
        "dev": "concurrently \"cd backend && pnpm run dev\" \"cd frontend && pnpm run dev\"",
        "prepack": "./scripts/build.sh"
    },
    "keywords": [
        "agent",
        "mcp",
        "container-use",
        "ui",
        "container",
        "dagger"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/b1tank/container-use-web.git"
    },
    "bugs": {
        "url": "https://github.com/b1tank/container-use-web/issues"
    },
    "homepage": "https://github.com/b1tank/container-use-web#readme",
    "author": "Zhichao Li <b1tank@outlook.com> (https://github.com/b1tank)",
    "license": "MIT",
    "files": [
        "bin/",
        "backend/dist/",
        "frontend/dist/"
    ],
    "dependencies": {
        "@hono/node-server": "^1.17.1",
        "@hono/node-ws": "^1.2.0",
        "@hono/swagger-ui": "^0.5.2",
        "@hono/zod-openapi": "^1.0.2",
        "chokidar": "^4.0.3",
        "hono": "^4.8.10",
        "node-pty": "^1.0.0",
        "zod": "^4.0.14",
        "commander": "^14.0.0",
        "open": "^10.1.0"
    },
    "devDependencies": {
        "@types/node": "^20.11.17",
        "concurrently": "^9.1.0",
        "esbuild": "^0.25.8",
        "typescript": "^5.8.3"
    },
    "engines": {
        "node": ">=20.0.0"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "@swc/core",
            "@tailwindcss/oxide",
            "esbuild",
            "node-pty"
        ]
    },
    "preferGlobal": true
}
